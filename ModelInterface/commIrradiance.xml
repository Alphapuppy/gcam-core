<?xml version="1.0" encoding="ISO-8859-1"?>
<commands>
	<variable name="population" type="reference">
		<region value="World" />
		<field value="PopulationDensity" />
		<time value="1995.0" />
	</variable>
	<variable name="irradiance" type="reference">
		<region value="World" />
		<field value="SolarIrradiance" />
		<time value="1993.0" />
	</variable>
	<variable name="forest" type="reference">
		<region value="World" />
		<field value="ForestCover" />
		<time value="1993.0" />
	</variable>
	
	<parseLessThan>
		<target name="forest" />
		<argument name="forest" />
		<limit value="80" />
	</parseLessThan>
	
	<plot variable="forest" />
	<plot variable="population" />
	
	<maskRemain>
		<target name="irradiance" />
		<argument name="irradiance" />
		<mask name="population" />
		<limit value="50" />
		<weight value="false" />
	</maskRemain>
	<maskRemove>
		<target name="irradiance" />
		<argument name="irradiance" />
		<mask name="forest" />
		<limit value="25" />
		<weight value="false" />
	</maskRemove>

	<forEachSubregion>
		<source name="irradiance" />
		<level value="3" />
		<command>
			<frequencyAnalysis>
				<output type="text" />
				<argument name="*" />
				<buckets number="20" />
			</frequencyAnalysis>
		</command>
	</forEachSubregion>
</commands>