<?xml version="1.0" encoding="ISO-8859-1"?>
<commands>
	<variable name="forest" type="reference">
		<region value="USAcontiguous" />
		<field value="National Forest FS" />
		<time value="2004.0" />
	</variable>
	<variable name="army" type="reference">
		<region value="USAcontiguous" />
		<field value="Army DOD" />
		<time value="2004.0" />
	</variable>
	<variable name="wind1" type="reference">
		<region value="USAcontiguous" />
		<field value="Class1" />
		<time value="2005.0" />
	</variable>
	<variable name="cost" type="reference">
		<region value="USAcontiguous" />
		<field value="DistCost" />
		<time value="2005.0" />
	</variable>

	<maskCombineOr>
		<target name="masterMask" />
		<argument name="army" />
		<argument name="forest" />
	</maskCombineOr>
	<maskRemove>
		<target name="maskWind" />
		<argument name="wind1" />
		<mask name="masterMask" />
		<weight value="true" />
	</maskRemove>
	
	<frequencyAnalysis>
		<output type="text" />
		<argument name="maskWind" />
		<split name="cost" />
		<buckets number="10" />
	</frequencyAnalysis>

	<print variable="maskWind" />
	<plot variable="cost" />
</commands>