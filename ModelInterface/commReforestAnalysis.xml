	</variable>
	<variable name="PotVegTempBROAD_DECIDUOUS" type="reference">
		<region value="World" />
		<field value="PotVeg5.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegBoreal_EVERGREEN" type="reference">
		<region value="World" />
		<field value="PotVeg6.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegBoreal_DECIDUOUS" type="reference">
		<region value="World" />
		<field value="PotVeg7.0" />
