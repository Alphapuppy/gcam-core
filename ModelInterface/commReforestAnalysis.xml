	</variable>
	<variable name="PotVegMIXED_FOREST" type="reference">
		<region value="World" />
		<field value="PotVeg8.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegSAVANNA" type="reference">
		<region value="World" />
		<field value="PotVeg9.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegGRASSLAND" type="reference">
		<region value="World" />
		<field value="PotVeg10.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegDENSE_SHRUBLAND" type="reference">
		<region value="World" />
		<field value="PotVeg11.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegOPEN_SHRUBLAND" type="reference">
		<region value="World" />
		<field value="PotVeg12.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegTUNDRA" type="reference">
		<region value="World" />
		<field value="PotVeg13.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegDESERT" type="reference">
		<region value="World" />
		<field value="PotVeg14.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegPOLAR" type="reference">
		<region value="World" />
		<field value="PotVeg15.0" />
		<time value="1990.0" />
	</variable>

	<add>
		<target name="PotentialForest" />
		<argument name="PotVegTropEVERGREEN" />
		<argument name="PotVegTropDECIDUOUS" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegTempBROAD_EVERGREEN" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegTempBROAD_DECIDUOUS" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegTempBROAD_EVERGREEN" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegTempNEED_EVERGREEN" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegTempBROAD_DECIDUOUS" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegBoreal_EVERGREEN" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegBoreal_DECIDUOUS" />
	</add>

	<add>
		<target name="PotentialForest" />
		<argument name="PotentialForest" />
		<argument name="PotVegMIXED_FOREST" />
	</add>

	<add>
		<target name="CurrentForestAndCrops" />
		<argument name="ForestCover" />
		<argument name="Crop" />
	</add>

	<parseLessThan>
		<target name="CurrentForestAndCrops" />
		<argument name="CurrentForestAndCrops" />
		<limit value="1" />
	</parseLessThan>

	<subtract>
		<target name="AdditionalForest" />
		<argument name="PotentialForest" />
		<argument name="CurrentForestAndCrops" />
	</subtract>

	<parseGreaterThan>
		<target name="AdditionalForest" />
		<argument name="AdditionalForest" />
		<limit value="0" />
	</parseGreaterThan>

	<extractSubRegion>
		<target name="aRegion" />
		<source name="AdditionalForest" />
		<shape value="USA" />
	</extractSubRegion> 

	<sumArea>
		<target name="summedValue" />
		<argument name="aRegion" />
	</sumArea>
	
	<print variable="summedValue" />

	<extractSubRegion>
		<target name="aRegion" />
		<source name="AdditionalForest" />
		<shape value="Canada" />
	</extractSubRegion> 

	<sumArea>
		<target name="summedValue" />
		<argument name="aRegion" />
	</sumArea>
	
	<print variable="summedValue" />
	
	<plot variable="PotentialForest"/>

</commands>
