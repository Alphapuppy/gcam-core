	</variable>
	<variable name="ForestCover" type="reference">
		<region value="World" />
		<field value="ForestCover" />
		<time value="1993.0" />
	</variable>
	

	<variable name="PotVegTropEVERGREEN" type="reference">
		<region value="World" />
		<field value="PotVeg1.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegTropDECIDUOUS" type="reference">
		<region value="World" />
		<field value="PotVeg2.0" />
		<time value="1990.0" />
	</variable>
	<variable name="PotVegTempBROAD_EVERGREEN" type="reference">
