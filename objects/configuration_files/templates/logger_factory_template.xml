<LoggerFactory xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="LoggerFactory.xsd">
	<Logger name="Basic" type="PlainTextLogger">
		<FileName>Log.txt</FileName>
		<printLogNest>1</printLogNest>
		<printLogWarningLevel>1</printLogWarningLevel>
		<printLogTimeStamp>1</printLogTimeStamp>
		<printLogDateStamp>1</printLogDateStamp>
		<printLogLineNumber>1</printLogLineNumber>
		<printLogFileName>1</printLogFileName>
		<printLogFullPath>1</printLogFullPath>
		<minLogWarningLevel>0</minLogWarningLevel>
		<minToScreenWarningLevel>0</minToScreenWarningLevel>
		<logTabSize>3</logTabSize>
		<headerMessage>This is the {date} basic {time} logger.</headerMessage>
	</Logger>
	<Logger name="Bug" type="PlainTextLogger">
		<FileName>bug.txt</FileName>
		<printLogNest>0</printLogNest>
		<printLogWarningLevel>0</printLogWarningLevel>
		<printLogTimeStamp>0</printLogTimeStamp>
		<printLogDateStamp>0</printLogDateStamp>
		<printLogLineNumber>0</printLogLineNumber>
		<printLogFileName>0</printLogFileName>
		<printLogFullPath>0</printLogFullPath>
		<minLogWarningLevel>0</minLogWarningLevel>
		<minToScreenWarningLevel>0</minToScreenWarningLevel>
		<logTabSize>3</logTabSize>
		<headerMessage>{date}This is the bug logger.{time}</headerMessage>
	</Logger>
	<Logger name="XML" type="XMLLogger">
		<FileName>log.xml</FileName>
		<printLogNest>0</printLogNest>
		<printLogWarningLevel>0</printLogWarningLevel>
		<printLogTimeStamp>0</printLogTimeStamp>
		<printLogDateStamp>0</printLogDateStamp>
		<printLogLineNumber>0</printLogLineNumber>
		<printLogFileName>0</printLogFileName>
		<printLogFullPath>0</printLogFullPath>
		<minLogWarningLevel>0</minLogWarningLevel>
		<minToScreenWarningLevel>0</minToScreenWarningLevel>
		<logTabSize>0</logTabSize>
		<headerMessage/>
	</Logger>
	<Logger name="ConfLogger" type="PlainTextLogger">
		<FileName>conf.log</FileName>
		<printLogNest>1</printLogNest>
		<printLogWarningLevel>1</printLogWarningLevel>
		<printLogTimeStamp>1</printLogTimeStamp>
		<printLogDateStamp>0</printLogDateStamp>
		<printLogLineNumber>1</printLogLineNumber>
		<printLogFileName>0</printLogFileName>
		<printLogFullPath>0</printLogFullPath>
		<minLogWarningLevel>0</minLogWarningLevel>
		<minToScreenWarningLevel>3</minToScreenWarningLevel>
		<logTabSize>3</logTabSize>
		<headerMessage>{date}: This is the Configuration class logger.</headerMessage>
	</Logger>
	<Logger name="SectorDependenciesLogger" type="PlainTextLogger">
		<FileName>SectorDependencies.csv</FileName>
		<printLogNest>0</printLogNest>
		<printLogWarningLevel>0</printLogWarningLevel>
		<printLogTimeStamp>0</printLogTimeStamp>
		<printLogDateStamp>0</printLogDateStamp>
		<printLogLineNumber>0</printLogLineNumber>
		<printLogFileName>0</printLogFileName>
		<printLogFullPath>0</printLogFullPath>
		<minLogWarningLevel>0</minLogWarningLevel>
		<minToScreenWarningLevel>5</minToScreenWarningLevel>
		<logTabSize>3</logTabSize>
		<headerMessage>{date}: All regions and their sector dependencies.</headerMessage>
	</Logger>
</LoggerFactory>
